{
  "expo": {
    "name": "DigiSchool",
    "slug": "digischool",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-copy.png",
    "splash": {
      "image": "./assets/mysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dipam.digischool"
    },
    "web": {
      "favicon": "./assets/logo-copy.png"
    },
    "android": {
      "package": "com.dipam.digischool"
    }
  }
}